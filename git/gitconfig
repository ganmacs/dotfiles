[user]
	useConfigOnly = true
[core]
	excludesfile = /Users/yuta-iwama/.gitignore_global
  editor = vim
  pager = less
[color]
  ui = auto
  diff = auto
[push]
  default = tracking
[ghq]
  root = ~/src
[alias]
  co = checkout
  p  = push
  d  = diff
  po = push origin
  cm = commit
  s  = status --short --branch
  o  = browse
  st = status
  ss  = stash
  pop  = stash pop
  br = branch
  re = rebase
  u  = remote update
  ft = fetch
  ri = rebase -i
  up = rebase origin/master
  uc = reset --mixed HEAD~1
  rc = commit -c ORIG_HEAD
  hist = log --graph --stat
  l = log --graph --all --format=format:'%C(bold red)%h%C(reset) - %C(bold green)(%ar)%C(reset) %C(white)%s%C(reset) %C(bold blue)â€• %an%C(reset)%C(bold yellow)%d%C(reset)' --abbrev-commit --date=relative
  lo = log --oneline
[filter "lfs"]
  clean = git lfs clean %f
  smudge = git lfs smudge %f
  required = true
[cmsg]
  ignorePaths=ganmacs
  ignoreCommits=ganmacs
[pager]
	log = diff-highlight | less
	show = diff-highlight | less
	diff = diff-highlight | less
[include]
  path = ~/.gitconfig.local
[hub]
  maintainerModify = true
[rebase]
	autostash = true
